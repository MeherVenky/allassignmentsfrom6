class Solution {
    public int[] plusOne(int[] digits) {
        
    int b[]=new int[digits.length];
        for(int i=0;i<digits.length;i++){
    if(i<digits.length-1){
    b[i]=digits[i];
   }
    else{
   b[i]=digits[i]+1;
   }
     }
for(int i=0;i<digits.length;i++){
System.out.print(b[i]);
}
return b;

        
    }
}
