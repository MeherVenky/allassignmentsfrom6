import java.util.*;
class Stack_Max_Min{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
System.out.println("Enter n value");
int n=sc.nextInt();
Stack<Integer> s1=new Stack<Integer>();
System.out.println("Enter elements into stack");
for(int i=0;i<n;i++){
s1.push(sc.nextInt());
}
System.out.println(s1);
System.out.println("Maximum Element in stack is = "+Collections.max(s1));
System.out.println("Minimum Element in stack is = "+Collections.min(s1));
}
}
