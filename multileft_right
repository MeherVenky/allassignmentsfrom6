class Complete{
    public static int multiply (int arr[], int n) {
           int s1=0;
           int s2=0;
           for(int i=0;i<n/2;i++){
              
                   s1=s1+arr[i];
                   
           } 
           for(int i=n/2;i<n;i++){
               
               s2=s2+arr[i];
           } 
           
           return s1*s2;
           
    }
    
    
}
