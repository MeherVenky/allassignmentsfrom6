import java.util.*;
class Zero_AtEnd
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int a[],n;
n=sc.nextInt();
a=new int[n];
int c=0;
for(int i=0;i<n;i++){
a[i]=sc.nextInt();
}

for(int i=0;i<n;i++){
if(a[i]!=0 ){
a[c++]=a[i];
}

}
while(c<n){
 
 a[c++]=0;
}

for(int i=0;i<n;i++){
System.out.print(a[i]);
}
}
}
    
           






